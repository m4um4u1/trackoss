{
  "name": "trackoss",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:run": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "playwright install",
    "cleanup:routes:list": "node e2e/scripts/cleanup-routes.js list",
    "cleanup:routes:all": "node e2e/scripts/cleanup-routes.js all",
    "cleanup:routes:test": "node e2e/scripts/cleanup-routes.js test",
    "format": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.1",
    "@angular/compiler": "^20.1.1",
    "@angular/core": "^20.1.1",
    "@angular/forms": "^20.1.1",
    "@angular/platform-browser": "^20.1.1",
    "@angular/platform-browser-dynamic": "^20.1.1",
    "@angular/router": "^20.1.1",
    "@maplibre/ngx-maplibre-gl": "^20.0.0",
    "bootstrap": "^5.3.6",
    "lucide-angular": "^0.525.0",
    "maplibre-gl": "^5.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.1",
    "@playwright/test": "^1.54.1",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-extended": "^6.0.0",
    "jest-preset-angular": "^14.6.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  }
}
